    "web-encoding": "^1.1.5"
  },
  "peerDependencies": {
    "@formily/core": "^2.1.3",
    "@formily/json-schema": "^2.1.3",
    "@formily/path": "^2.1.3",
    "@toy-box/meta-schema": "^3.0.2",
    "@toy-box/toybox-shared": "^3.0.2"
  },
  "devDependencies": {
    "@formily/core": "^2.1.3",
    "@formily/json-schema": "^2.1.3",
    "@formily/path": "^2.1.3",
    "@toy-box/meta-schema": "^3.0.2",
